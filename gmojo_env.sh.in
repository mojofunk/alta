
BUILD_DIR=%BUILD_DIR%

GLEAM_LIBRARY_PATH=$BUILD_DIR/libgleam

MOJO_LIBRARY_PATH=$BUILD_DIR/mojo

MOJO_PLUGIN_LIBRARY_PATH=$BUILD_DIR/mojo/plugins

DUMMY_ARCHIVE_PLUGIN_LIBRARY_PATH=$MOJO_PLUGIN_LIBRARY_PATH/dummy_archive

export LD_LIBRARY_PATH=\
$GLEAM_LIBRARY_PATH:\
$MOJO_LIBRARY_PATH:\
$MOJO_PLUGIN_LIBRARY_PATH:\
$DUMMY_ARCHIVE_PLUGIN_LIBRARY_PATH

export EXECUTABLE="%BUILD_DIR%/ui/%VERSIONED_NAME%"
